{
    "env": {
        "node": true,
        "browser": true,
        "jest": true,
        "es6": true 
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1,{ "extensions": [ ".js", ".jsx" ]}],
        "import/no-extraneous-dependencies": [ "warn", {
            "devDependencies": true,
            "optionalDependencies": false,
            "peerDependencies": false
        }],
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": []
        }]
    }
  }